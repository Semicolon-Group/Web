            $('.next_step').click(function () {
                if(validFirst()){
                    $('#first').hide();
                    $('#step_one').hide();
                    $('#second').show();
                    $('#step_two').show();
                }
            })

            $('.scd_step').click(function () {

            })

            $('.bck_scd_step').click(function () {
                $('#first').show();
                $('#step_one').show();
                $('#second').hide();
                $('#step_two').hide();
            })

            function validFirst() {
                $('.first-control').each(function (i, v) {
                    if(!v.checkValidity())
                        return false;
                })
                return true;
            }
