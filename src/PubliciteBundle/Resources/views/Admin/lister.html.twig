{% block body %}
    {% for m in pubs %}
        <table border="1">

            <tr>
                <th>business id </th>
                <th>content</th>
                <th>reason</th>
                <th>price</th>
                <th>payed</th>
            </tr>

            <tr>
                <td>{{ m.business }}</td><td>{{ m.content }}</td><td>{{ m.reason }}</td><td>{{ m.price }}</td><td>{% if m.payed==1 %} Yes {% else %} No {% endif %}</td>
                <td>    <a href="{{ path('traiter_admin', {'id':m.id}) }}">Modifier</a></td>
                <td>    <a href="{{ path('supprimer_admin', {'id':m.id}) }}">Supprimer</a></td>

            </tr>
        </table>
    {% endfor %}

{% endblock %}
